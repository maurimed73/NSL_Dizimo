<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.CedulasActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Relatório"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/border"
        android:padding="3dp"
        android:stretchColumns="0,1,2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_marginBottom="5dp"
            android:background="#F8F3F3">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center_vertical"
                android:text="Notas R$ 200,00"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtQuant200"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtValor200"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:textSize="15sp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_marginBottom="5dp">

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center_vertical"
                android:text="Notas R$ 100,00"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtQuant100"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtValor100"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:textSize="15sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="5dp"
            android:background="#F8F3F3">

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center_vertical"
                android:text="Notas R$ 50,00"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtQuant50"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:text=""
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtValor50"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:text=""
                android:textSize="15sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="5dp">

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center_vertical"
                android:text="Notas R$ 20,00"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtQuant20"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:text=""
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtValor20"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:text=""
                android:textSize="15sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="5dp"
            android:background="#F8F3F3">

            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center_vertical"
                android:text="Notas R$ 10,00"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtQuant10"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:text=""
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtValor10"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:text=""
                android:textSize="15sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="5dp">

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center_vertical"
                android:text="Notas R$ 5,00"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtQuant5"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:text=""
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtValor5"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:text=""
                android:textSize="15sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="5dp"
            android:background="#F8F3F3">

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center_vertical"
                android:text="Notas R$ 2,00"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtQuant2"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtValor2"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:textSize="15sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center_vertical"
                android:text="Qtd Moedas"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtMoedas1"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/txtMoedas2"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center|center_vertical"
                android:textSize="15sp" />
        </TableRow>

    </TableLayout>

    <TextView
        android:id="@+id/txtValorDi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="12dp"
        android:text="Valor Dízimo"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout" />

    <TextView
        android:id="@+id/txtDataDizimo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="12dp"
        android:lineSpacingExtra="14sp"
        android:text="Data "
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/txtValorDi"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout" />

    <TextView
        android:id="@+id/txtValorDizimo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="12dp"
        android:text="valor"
        android:textSize="16sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/txtDataDizimo"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="50dp"
        android:backgroundTint="@color/design_default_color_secondary"
        android:onClick="voltarInicio"
        android:src="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voltar"
        app:layout_constraintBottom_toTopOf="@+id/imageButton"
        app:layout_constraintEnd_toEndOf="@+id/imageButton"
        app:layout_constraintStart_toStartOf="@+id/imageButton" />

    <View
        android:id="@+id/divider2"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtValorDi" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="207dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider2">

        <ListView
            android:id="@+id/lstColetas"
            android:layout_width="217dp"
            android:layout_height="202dp"
            android:layout_marginStart="24dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/button"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="5dp"
            android:text="Coletas"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>